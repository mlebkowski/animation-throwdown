<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="Nassau\CartoonBattle\Entity\Game\UserGatherRumbleStats" table="game_user_gather_rumble_stats">

        <id name="id" type="string" column="id">
            <generator strategy="UUID"/>
        </id>

        <field name="enabled" type="boolean" nullable="false"/>

        <many-to-one target-entity="User" field="user">
            <join-column nullable="false" name="user_id"/>
        </many-to-one>

    </entity>

</doctrine-mapping>
